{
    "version": 2,
    "routes": [{
        "src": "/api/products",
        "dest": "server/api/products/index.js"
    }],
    "builds": [{
        "src": "server/api/**/*.js",
        "use": "@vercel/node"
    }, {
        "src": "nuxt.config.ts",
        "use": "@nuxtjs/vercel-builder",
        "config": {
            "serverFiles": ["server/**"],
            "tscOptions": {
                "sourceMap": false
            }
        }
    }]
}